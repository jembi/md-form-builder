{
  "name": "safer-surgery-portal",
  "private": true,
  "version": "0.1.0",
  "description": "Safe Surgery South Africa",
  "scripts": {
    "build": "browserify --entry app/scripts/app.js -t [ babelify --presets [ es2015 ] ] | uglifyjs --output dist/bundle.js",
    "start": "npm link md-form-builder && npm run build && light-server -s . -p 9500 -w \"node_modules/md-form-builder/dist/**, app/**, index.html # npm run build # reload\"",
    "test": "npm run -s test:code && npm run -s test:style",
    "test:code": "tap -R nyan --cov test/**/*.js test/**/**/*.js test/**/**/**/*.js",
    "test:these-files": "tap --reporter=spec",
    "test:style": "standard --verbose | snazzy && htmlhint app index.html",
    "test:debug": "tap --debug -t 0 test/**/*.js",
    "test:these": "tap -R nyan",
    "cov": "npm run test:code -- --coverage-report=lcov"
  },
  "main": "app/scripts/app.js",
  "repository": "git@github.com:jembi/safer-surgery-portal.git",
  "author": "Jembi Health Systems NPC",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "htmlhint": "^0.9.13",
    "light-server": "^2.0.2",
    "sinon": "^3.2.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tap": "^10.3.2",
    "uglify-js": "^3.0.11"
  },
  "dependencies": {
    "ace-builds": "^1.2.8",
    "angular": "^1.6.5",
    "angular-animate": "^1.6.5",
    "angular-aria": "^1.6.5",
    "angular-chart.js": "^0.10.2",
    "angular-cookies": "^1.6.5",
    "angular-google-maps": "2.3.2",
    "angular-material": "^1.1.4",
    "angular-material-data-table": "^0.10.9",
    "angular-messages": "^1.6.5",
    "angular-recaptcha": "^3.0.3",
    "angular-resource": "^1.6.5",
    "angular-route": "^1.6.5",
    "angular-timeline": "^1.7.0",
    "crypto-js": "^3.1.9-1",
    "js-base64": "^2.1.9",
    "moment": "^2.18.1",
    "ng-prettyjson": "^0.2.0",
    "ngstorage": "^0.3.11",
    "pdfmake": "^0.1.32",
    "uuid": "^3.1.0"
  }
}
